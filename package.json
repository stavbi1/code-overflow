{
	"name": "code-overflow",
	"displayName": "code-overflow",
	"description": "A vs code extension for searching and integrating with stack overflow",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:code-overflow.search-stackoverflow",
		"onView:questions"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "code-overflow.search-stackoverflow",
				"title": "search stackoverflow"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "code-overflow.search-stackoverflow",
					"group": "editor"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "code-overflow",
					"title": "code overflow",
					"icon": "media/code-overflow.svg"
				}
			]
		},
		"views": {
			"code-overflow": [
				{
					"id": "questions",
					"name": "questions",
					"type": "webview"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "rollup -c && webpack --mode production",
		"watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^14.0.0",
		"@rollup/plugin-node-resolve": "^8.0.0",
		"@rollup/plugin-typescript": "^6.0.0",
		"@tsconfig/svelte": "^1.0.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"concurrently": "^5.3.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"rollup": "^2.3.4",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.0",
		"svelte": "^3.0.0",
		"svelte-check": "^1.0.0",
		"svelte-preprocess": "^4.0.0",
		"ts-loader": "^8.0.11",
		"typescript": "^4.0.2",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"open": "^8.4.0"
	}
}
